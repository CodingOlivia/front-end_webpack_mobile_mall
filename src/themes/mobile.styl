//@font-face
  //font-family: 'Khmer MN'
  //src: url('~statics/font/Khmer-MN.ttc')
@font-face
  font-family: 'Helvetica Neue'
  src: url('~statics/font/Helvetica-Neue.ttf')
@font-face
  font-family: 'HelveticaNeue'
  src: url('~statics/font/HelveticaNeue.ttf')
@font-face
  font-family: 'HelveticaNeue Bold'
  src: url('~statics/font/HelveticaNeue-Bold.otf')
$font-family-K-M = 'Khmer MN'
// $font-family-H-N = 'Helvetica Neue'
// $font-family-HN = 'HelveticaNeue'
@import '~themes/app.variables'
* {
  user-select none
}
html[app=mobile], html[app=mobile] body
  font-size calc(10000vw / 750)
  font-family HelveticaNeue
html[app=mobile] body
  ul, li, ol
    list-style none
    padding 0
    margin 0
  a
    color #252525
    &:hover
    	color #252525
  i
    display flex
    justify-content center
    align-items center
    font-size 0
  input
    outline none
    border none
    user-select text
  .transition
    transition all .3s ease-in
  .transition-linear
    transition all .3s linear
  .text-light-black
    color #252525 !important
  // home start
  .mobileHome
    width 100vw
    max-width 100%
    min-height 100vh
    .TopBanner
      width 100%
      height 100vh
      position relative
      display flex
      overflow hidden
      .homeBanner
        width 100%
        height 100vh
        position relative
        display flex
        overflow hidden
        .bannerBg
          width 100%
          height 100%
          position relative
          flex-shrink 0
          >img
            pointer-events none
          .bannerAttr
            height 1.2rem
            display flex
            flex-direction column
            position absolute
            left .4rem
            bottom 2.08rem
            z-index 100
            >div
              height .6rem
              display flex
              // font-family 'Helvetica Neue'
              &:first-child
                align-items flex-start;
              &:last-child
                align-items flex-end;
            .attrName
              color $all-color-1
              font-size .64rem
              font-family 'HelveticaNeue Bold'
            .attrPrice
              color $all-color-1
              font-size .32rem
            .attrType
              padding-left .2rem
              color $all-color-2
              font-size .32rem
      .bannerPage
        display flex
        justify-content space-between
        position absolute
        right .4rem
        bottom 1.5rem
        width 1.52rem
        align-items center
        >span:first-child
          color #252525
          font-size .32rem
          font-weight bold
        >div
          width .64rem
          height .04rem
          background #fff
        >span:last-child
          color #fff
          font-size .32rem
          font-weight bold
      .tools
        width calc(100% - .8rem)
        height .64rem
        display flex
        justify-content space-between
        position absolute
        left .4rem
        right .4rem
        bottom .5rem
        z-index 200
        >div
          display flex
          font-family 'HelveticaNeue Bold'
          &:first-child>span
            width 2rem
            height .64rem
            color $all-color-1
            font-size .24rem
            background #f3f9fc
            display flex
            justify-content center
            align-items center
            border-radius .32rem
            cursor pointer
          &:last-child>span
            width .64rem
            height .64rem
            cursor pointer
          .p-lt
            background url('~statics/mobileImages/home/ico_left.png') center no-repeat
            background-size 100% 100%
            margin-right .24rem
            &:hover
              background url('~statics/mobileImages/home/ico_left2.png') center no-repeat
              background-size 100% 100%
          .p-rt
            background url('~statics/mobileImages/home/ico_right.png') center no-repeat
            background-size 100% 100%
            &:hover
              background url('~statics/mobileImages/home/ico_right2.png') center no-repeat
              background-size 100% 100%
    .NewProducts, .FeaturedProducts
      width 100%
      display flex
      flex-direction column
      color $all-color-1
      .title
        height 1.22rem
        padding-left .36rem
        font-size .26rem
        font-family 'HelveticaNeue Bold'
        display flex
        align-items center
      .content
        width 100%
        display flex
    .NewProducts>.content
      width calc(100% - .72rem)
      margin 0 .36rem
      overflow-y hidden
      overflow-x auto
      >.new
        width calc(50% - .08rem)
        display flex
        flex-shrink 0
        flex-direction column
        align-items center
        box-sizing border-box
        border 1px solid $border-color-1
        margin 0 .16rem 0 0
        &:last-child
          margin-left 0
        &:hover
          border .02rem solid $all-color-1
          transition-duration 2s
        >div
          width 100%
          display flex
          align-items center
          justify-content center
          &:nth-child(1)
            height 3.6rem
            >img
              width auto
              height 2.76rem
          &:nth-child(2)
            width 70%
            height 1.2rem
            align-items flex-start
            >.newName
              // width 100%
              height 1rem
              font-size .28rem
              font-family 'HelveticaNeue Bold'
              color $all-color-1
              display -webkit-box
              /* autoprefixer: off */ 
              -webkit-box-orient vertical
              /* autoprefixer: on */
              -webkit-line-clamp 3
              overflow hidden
              text-align center
          &:nth-child(3)
            width 70%
            height 1rem
            align-items flex-start
            >.newPrice
              // width 100%
              // height 1rem
              font-size .28rem
              // font-family 'Helvetica Neue'
              color $all-color-1
              text-align center
              padding-right .24rem
              font-weight 600
            >.newType
              // width 100%
              // height 1rem
              font-size .28rem
              // font-family 'Helvetica Neue'
              color $font-color-3
              text-align center
              font-weight 600
    .FeaturedProducts>.content
      flex-direction column
      .featured
        width 100%
        height 111.333vw
        display flex
        flex-direction column
        background $all-color-2
        >div
          width 100%
          display flex
          flex-direction column
          &:first-child
            width 100vw
            height 53.333vw
            justify-content center
            text-align center
            border-bottom 1px solid #f1f1f1
          &:last-child
            width 93.333%
            height 62vw
            background $all-color-2
            margin -.46rem auto 0
            border 1px solid #f1f1f1
        .featuredAttr
          display flex
          // padding .32rem .48rem
          padding 4.266vw 6.4vw
          >div
            display flex
            text-align center
            >span
              display flex
              text-align center
              font-weight 600
            &:nth-child(1)
              width 100%
              height .48rem
              font-size .32rem
              // font-family 'Helvetica Neue'
              font-weight bold
            &:nth-child(2)
              width 100%
              height .56rem
              font-size .28rem
              // font-family 'Helvetica Neue'
            &:nth-child(3)
              width 100%
              height 1.32rem
              margin-top .16rem
              font-size .28rem
              line-height .32rem
              // font-family 'Helvetica Neue'
              color $all-color-3
              >.featuredDesc
                width 100%
                height 100%
                text-align left
                display -webkit-box
                font-weight 400
                /* autoprefixer: off */ 
                -webkit-box-orient vertical
                /* autoprefixer: on */
                -webkit-line-clamp 4
                overflow hidden
            &:nth-child(4)
              width 100%
              height .64rem
              margin-top .64rem
              font-size .28rem
              line-height .32rem
              // font-family 'Helvetica Neue'
              font-weight bold
              justify-content space-between
              >span
                display flex
                justify-content center
                align-items center
                border-radius .32rem
                cursor pointer
                &.explore
                  width 40.533vw
                  max-width 50%
                  height .64rem
                  border 2px solid $border-color-1
                  background $all-color-2
                  color $all-color-1
                &.buy
                  width 35.333vw
                  max-width 50%
                  height .64rem
                  border 2px solid $all-color-1
                  background $all-color-1
                  color $all-color-2
    .seeMore
      width 100%
      height 2.76rem
      border-bottom 1px solid $border-color-1
      display flex
      justify-content center
      align-items center
      >a
        width 6.06rem
        height .94rem
        background $all-color-1
        border-radius .47rem
        font-size .32rem
        // font-family 'Helvetica Neue'
        color $all-color-2
        display flex
        justify-content space-between
        align-items center
        padding 0 1.1rem
    .other
      width 100%
      display flex
      flex-direction column
      >div
        display flex
        justify-content center
        align-items center
        >span
          width 90%
          text-align center
          color $all-color-1
        &:nth-child(1)
          height .48rem
          margin-top .48rem
          font-size .32rem
          font-weight bold
          // font-family 'Helvetica Neue'
        &:nth-child(2)
          height .48rem
          margin-bottom .48rem
          font-size .26rem
          // font-family 'Helvetica Neue'
          color $all-color-3
        &:nth-child(3)
          font-size .26rem
          font-weight bold
          // font-family 'Helvetica Neue'
          border 2px solid $border-color-1
          border-radius .47rem
      .inputDiv
        width 6.3rem
        height .94rem
        margin 0 auto 1.6rem
        >div
          display flex
          align-items center
          &:nth-child(1)
            width 4.32rem
            height 100%
            padding-left .36rem
            color $all-color-1
            >input
              width 100%
          &:nth-child(2)
            width 1.9rem
            height 100%
            color $all-color-1
            justify-content center
            border-left 2px solid $border-color-1
            border-radius 0 .47rem .47rem 0
            &:active
              background rgba(37, 37, 37, .1)
          .ico-infoBtn
            width .12rem
            height .2rem
            font-size 0
            margin-left .18rem
            background url('~statics/mobileImages/home/ico_arrow_R.png') center no-repeat
            background-size 100% 100%
  // home end
  // checkout-address start
  .addressDiv
    margin-top 1.76rem
    font-size .26rem
    position relative
    width 100%
    padding 0 .24rem
    .none
      background none !important
      cursor auto !important
    >.address
      >.main
        height 1.5rem
        padding 0 .16rem
        display flex
        justify-content space-between
        align-items center
        border-bottom 2px solid $border-color-1
        &:first-child
          border-top 2px solid $all-color-1
        >.left
          max-width calc(100% - .64rem)
          display flex
          align-items center
          font-size .28rem
          font-weight bold
          color $all-color-1
          >.hide
            opacity .3
          >i
            width .48rem
            height .48rem
            margin .08rem
            flex-shrink 0
            cursor pointer
            background url('~statics/mobileImages/address/ico_default.png') center / 100% no-repeat
          >.content
            width calc(100% - .88rem)
            margin-left .24rem
            display flex
            justify-content space-between
            // align-items center
            flex-direction column
            >div
              display flex
              align-items center
              padding .1rem 0
              >span
                overflow hidden
                text-overflow ellipsis
                white-space nowrap
              >.name, >.number
                font-size .28rem
                font-weight bold
                color $all-color-1
              >.address
                font-size .26rem
                font-weight normal
                color $all-color-3
        >i
          width .48rem
          height .48rem
          margin-left .16rem
          flex-shrink 0
          cursor pointer
          background url('~statics/mobileImages/address/ico_edit.png') center / 100% no-repeat
        >.top
          width 100%
          height .64rem
          margin-top -.64rem
          >ul
            display flex
            >li
              font-size .26rem
              color $all-color-3
              margin-right .6rem
              cursor pointer
            >.active
              font-weight bold
              color $all-color-1
        >.lableDiv
          height .92rem
          margin .16rem 0
          padding-left .32rem
          border 2px solid $border-color-1
          display flex
          justify-content space-between
          align-items center
          position relative
          >.error
            position absolute
            right .2rem
            color #f00
            font-weight bold
          >input
            width 100%
          >i
            width .48rem
            height .48rem
            flex-shrink 0
            margin 0 .2rem
            cursor pointer
            background url('~statics/mobileImages/sign/reset.png') center / 100% no-repeat
          >div
            width 2rem
            height 100%
            flex-shrink 0
            margin-left .2rem
            padding .16rem
            border-left 2px solid $border-color-1
            display flex
            justify-content center
            align-items center
            cursor pointer
            &:active
              background rgba(37, 37, 37, .1)
        >.signIn
          width 100%
          padding .24rem 0
          >span
            width 100%
            height .96rem
            margin .32rem 0 .28rem
            font-size .32rem
            border-radius .48rem
            background $all-color-1
            color $all-color-2
            // font-weight bold
            display flex
            justify-content center
            align-items center
            cursor pointer
        >.signTip
          width 100%
          display flex
          justify-content space-between
          align-items center
          >span
            color #66bbff
            &:last-child
              color $all-color-1
              font-weight bold
              cursor pointer
              display flex
              justify-content space-between
              align-items center
              >i
                width .12rem
                height .2rem
                margin 0 .16rem
                background url('~statics/mobileImages/sign/ico_arrow.png') center / 100% no-repeat
  // checkout-address end
  .addressTableDiv
    // margin-top 1.76rem
    margin-top 1.28rem
    font-size .26rem
    position relative
    width 100%
    padding .38rem .24rem
    >.checkHr
      width 100%
      padding 1.6rem .24rem 0
      background #fff
      box-sizing border-box
      position fixed
      top 0
      left 0
      z-index 300
      >span
        width 100%
        height 100%
        display flex
        border-bottom 2px solid $all-color-1
    .none
      background none !important
      cursor auto !important
    >.table
      // border-top 2px solid #252525
      padding .38rem .16rem 0
      >.main
        margin-bottom .38rem
        border .02rem solid $border-color-1
        box-sizing border-box
        &:last-child
          margin-bottom .28rem
        >div
          width 100%
          height .94rem
          box-sizing border-box
          border-bottom .02rem solid $border-color-1
          display flex
          align-items center
          >div
            width 100%
            height 100%
            margin 0
            padding 0 .22rem 0 .34rem
            font-size .26rem
            color #666
            .q-if-control
              width .4rem
              height .4rem
              font-size 0
              background url('~statics/mobileImages/checkOut/ico_down.png') center / 100% no-repeat
          &:last-child
            border-bottom none
          >input
            width calc(100% - .6rem)
            // width 100%
            padding 0 0 0 .4rem
            font .26rem HelveticaNeue
          >input::-webkit-input-placeholder
            color #b2b2b2
          >i
            width .4rem
            height .4rem
            cursor pointer
            background url('~statics/mobileImages/address/ico_close.png') center / 100% 100% no-repeat
          >.select-box
            display flex
            align-items center
            height 100%
            width 100%
            justify-content space-between
            >span
              font-size .26rem
              color #b2b2b2
            >.select-icon
              display flex
              align-items center
              >img
                width .4rem
                height .4rem
        >.mapBtn
          display flex
          justify-content space-between
          align-items center
          >div
            width auto
            display flex
            justify-content space-between
            align-items center
          >.name
            >i
              width .32rem
              height .34rem
              background url('~statics/mobileImages/address/ico_tab.png') center / 100% no-repeat
            >span
              font-size .26rem
              color #666
              margin-left .24rem
          >.btn
            font-size 0
            .q-toggle .q-option-inner
              width 1.04rem
              height .64rem
              padding 0
              .q-toggle-base
                height 100%
                border-radius .32rem
                color #fff
                opacity 1
                box-sizing border-box
                border .03rem solid #e5e5e5
              .q-toggle-handle
                width .56rem
                height .56rem
                top .03rem
                left .03rem
                background-color #fff
            .q-toggle .q-option-inner.active .q-toggle-base
              color #62dda9
              border .03rem solid #62dda9
            .q-toggle .q-option-inner.active .q-toggle-handle
              left .45rem
              background-color #fff
        >.select-sex
          >div
            margin-right .78rem
            display flex
            align-items center
            cursor pointer
            >i
              width .48rem
              height .48rem
              border-radius 50%
              border 2px solid $all-color-3
              &.active
                border none
                background url('~statics/mobileImages/address/ico_default.png') center / 100% 100% no-repeat
            >span
              margin-left .24rem
        >.add-address
          display flex
          align-items center
          cursor pointer
          >i
            width .32rem 
            height .34rem
            flex-shrink 0
          >span
            width 100%
            padding-left .3rem
            color #b2b2b2
            white-space nowrap
            text-overflow ellipsis
            overflow hidden
          >.icon-tab
            background url('~statics/mobileImages/address/ico_tab.png') center / 100% 100% no-repeat
          >.icon-right
            background url('~statics/mobileImages/address/ico_right.png') center / 100% 100% no-repeat
      >.check-default
        display flex
        align-items center
        >i
          width .48rem
          height .48rem
          border-radius 50%
          cursor pointer
          opacity .3
          background url('~statics/mobileImages/address/ico_default.png') center / 100% 100% no-repeat
          &.active
            opacity 1
        >span
          padding-left .24rem
          font .26rem HelveticaNeue
          color #6abcfc
      >.btn-save
        width 100%
        height .96rem
        margin-top .6rem
        background #252525
        border-radius .48rem
        font .32rem HelveticaNeue-Bold
        color #fff
        display flex
        justify-content center
        align-items center
        cursor pointer
  .addressMapDiv
    margin-top 1.28rem
    font-size .26rem
    position relative
    width 100%
    .none
      background none !important
      cursor auto !important
    >.map
      width 100%
      box-sizing border-box
      border-top 1px solid #b2b2b2
      >.title
        width 100%
        height .64rem
        padding 0 .32rem
        display flex
        justify-content space-between
        align-items center
        >div
          display flex
          justify-content space-between
          align-items center
          >i
            width .29rem
            height .34rem
            background url('~statics/mobileImages/address/ico_map_tab.png') center / 100% 100% no-repeat
          >span
            font bold .26rem HelveticaNeue
            margin-left .24rem
        >i
          width .4rem
          height .4rem
          cursor pointer
          background url('~statics/mobileImages/address/ico_down.png') center / 100% 100% no-repeat
      >.gdmap
        width 100%
        height 5.54rem
        border-top 1px solid #b2b2b2
        box-sizing border-box
        background url('~statics/mobileImages/address/map.png') 0 0 / 100% 5.54rem no-repeat
        &.hide-map
          width 100%
          height 1px
          border-top 1px solid #b2b2b2
          box-sizing border-box
      >.address-lists
        height calc(100vh - 7.46rem - 2px)
        padding 0 .4rem
        border-top 2px solid $all-color-1
        display flex
        flex-direction column
        overflow-y auto
        &.hide-map
          height calc(100vh - 1.92rem - 2px)
        &:last-child
          border-top none
        >.left
          width 100%
          height 1.5rem
          flex-shrink 0
          display flex
          align-items center
          font-size .28rem
          font-weight bold
          color $all-color-1
          box-sizing border-box
          border-bottom 2px solid $border-color-1
          &:last-child
            border-bottom none
          >.hide
            opacity .3
          >i
            width .32rem
            height .34rem
            margin .08rem .08rem .6rem
            flex-shrink 0
            cursor pointer
            &.active
              background url('~statics/mobileImages/address/ico_tab.png') center / 100% no-repeat
          >.content
            width calc(100% - .88rem)
            margin-left .24rem
            display flex
            justify-content space-between
            // align-items center
            flex-direction column
            >div
              display flex
              align-items center
              padding .1rem 0
              >span
                overflow hidden
                text-overflow ellipsis
                white-space nowrap
              >.name, >.number
                font-size .28rem
                font-weight bold
                color $all-color-1
              >.address
                font-size .26rem
                font-weight normal
                color $all-color-3
  // checkout start
  .checkOutDiv
    // margin-top 1.76rem
    margin-top 1.28rem
    font-size .28rem
    position relative
    width 100%
    padding .32rem .24rem 0
    >.checkHr
      width 100%
      padding 1.6rem .24rem 0
      background #fff
      box-sizing border-box
      position fixed
      top 0
      left 0
      z-index 300
      >span
        width 100%
        height 100%
        display flex
        border-bottom 2px solid $all-color-1
    >.checkOutMain
      position relative
      z-index 200
      border 1px solid $border-color-1
      // border-top 2px solid $all-color-1
      box-shadow 0 .2rem .4rem rgba(0, 0, 0, .1)
      margin-bottom .4rem
      >.mySelfDiv
        width 100%
        height 1.6rem
        padding 0 .32rem
        margin-top -.48rem
        display flex
        justify-content space-between
        align-items center
        >div
          display flex
          >.heardImage
            width 1.6rem
            height 1.6rem
            border-radius 50%
            margin 0 .2rem
          >.info
            width 3rem
            display flex
            flex-direction column
            justify-content flex-end
            >div
              &:last-child
                width 100%
                height .16rem
                margin .32rem 0 .1rem
                background #f5f5f5
                border-radius .08rem
              >.infoName
                font-weight bold
              >.inforank
                background $all-color-1
                color $all-color-2
                font-weight bold
                border-radius .04rem
                margin-left .24rem
                padding .08rem .12rem
              >.rankProgress
                display flex
                height 100%
                background $background-color-4
                border-radius .08rem
        >i
          width .48rem
          height .48rem
          cursor pointer
      >.menu
        display flex
        flex-direction column
        padding-top .2rem
        >li
          display flex
          justify-content space-between
          align-items center
          padding 0 .16rem
          &:first-child>a
            height 1.50rem
            padding-bottom .28rem
          &:last-child>a
            border-bottom none
          >i
            width .48rem
            height .48rem
          >a
            width 100%
            height 1rem
            padding-left .08rem
            padding-bottom 0
            border-bottom 2px solid $border-color-1
            display flex
            justify-content space-between
            align-items center
            >.address
              width 6.4rem
              height .96rem
              display flex
              align-items center
              justify-content center
              border .02rem solid #e5e5e5
              border-radius 50px
              background #fff
              margin 0 .16rem
              >span
                font-size .32rem
                color #ff2973
                font-weight bold
                letter-spacing .02rem
                text-transform Uppercase
              >i
                  width .36rem
                  height .36rem
                  flex-shrink 0
                  margin-left .20rem
                  background url('~statics/mobileImages/checkOut/more.png') center / 100% no-repeat
            >span
              display flex
              justify-content space-between
              align-items center
